{
	"ConnectionStrings": {
		"Sqlite": "Data Source=/app/data/DataSpecificationNavigatorDB.db"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"DataSpecificationNavigator": "Trace"
		}
	},
	"Dataspecer": {
		"Endpoints": {
			"DownloadDocumentation": "https://tool.dataspecer.com/api/experimental/output.zip?iri="
		}
	},
	"Llm": {
		"Gemini": {
			"ApiKeyFile": "./Secrets/Gemini_api-key.txt",
			"Model": "models/gemini-2.5-flash",
			"RetryAttempts": 3,
			"Prompts": {
				"BaseDirectory": "./Prompts/Gemini",
				"ItemsMapping": "map_to_data_specification.txt",
				"GetRelatedItems": "get_suggested_items.txt",
				"GenerateSuggestedMessage": "generate_suggested_message.txt",
				"DataSpecSubstructureItemsMapping": "map_to_substructure.txt",
				"WelcomeMessageDataSpecificationSummary": "welcome_message_data_specification_summary.txt",
				"SummarizeDataSpecItems": "summarize_data_specification_classes.txt"
			}
		},
		"Ollama": {
			"Uri": "http://host.docker.internal:11434",
			"Model": "llama3.3:70b",
			"RetryAttemps": 3,
			"Prompts": {
				"BaseDirectory": "./Prompts/Llama3.3-70b",
				"ItemsMapping": "map_to_data_specification.txt",
				"GetRelatedItems": "get_suggested_items.txt",
				"GenerateSuggestedMessage": "generate_suggested_message.txt",
				"DataSpecSubstructureItemsMapping": "map_to_substructure.txt",
				"WelcomeMessageDataSpecificationSummary": "welcome_message_data_specification_summary.txt",
				"SummarizeDataSpecItems": "summarize_data_specification_classes.txt"
			}
		}
	}
}
